    "@angular/cli": "^18.0.7",
    "@angular/compiler-cli": "^18.0.0",
    "@ngrx/eslint-plugin": "^18.0.2",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
